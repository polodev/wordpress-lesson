body{color:#333}.center{max-width:1350px;padding:0 12px;margin:0 auto}.toc{margin:2em 0;padding:1em 0;border:1px solid #eee;border-radius:4px;box-shadow:0 1px 6px rgba(0,0,0,0.03)}.toc h2{margin-left:1em;border-bottom:1px solid beige}.toc h2 span{font-size:14px;display:inline-block;cursor:pointer}span.mark{color:brown}a{color:cadetblue}a:hover{color:#4c7e80}.search_result_box{position:relative}.search_result_box #search_result{position:absolute;top:100%;width:200px;box-sizing:border-box;right:0;z-index:1;display:none;border:1px solid #eee;background:cadetblue;border-radius:4px;box-shadow:0 1px 6px rgba(0,0,0,0.03)}.search_result_box #search_result ul{margin:0;padding:0;list-style:none}.search_result_box #search_result li{border-bottom:1px solid #eee;padding:5px;list-style:none;margin:0}.search_result_box #search_result a{color:#fff;display:block}

